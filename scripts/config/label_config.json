{
  "version": "1.0",
  "label_definitions": {
    "grade_double": {
      "id": "grade_double",
      "name": "学年ラベル（2つ記載）",
      "patterns": [
        "^〔第[1-6１-６]学年及び第[1-6１-６]学年〕$"
      ],
      "examples": [
        "〔第１学年及び第２学年〕"
      ],
      "description": "特殊処理用"
    },
    "grade_single": {
      "id": "grade_single",
      "name": "学年ラベル（1つ記載）",
      "patterns": [
        "^〔第[1-6１-６]学年〕$"
      ],
      "examples": [
        "〔第１学年〕"
      ],
      "description": "特殊処理用"
    },
    "instruction": {
      "id": "instruction",
      "name": "指導項目括弧",
      "patterns": [
        "^[〔【]指導項目[〕】]$"
      ],
      "examples": [
        "〔指導項目〕",
        "【指導項目】"
      ],
      "description": "Itemレベル（特殊処理）"
    },
    "subject_label": {
      "id": "subject_label",
      "name": "括弧科目名",
      "patterns": [
        "^[〔【［（].+[〕】］）]$"
      ],
      "examples": [
        "〔医療と社会〕",
        "【医療と社会】",
        "［善悪の判断，自律，自由と責任］",
        "（相談窓口の担当者が適切に対応することができるようにしていると認められる例）"
      ],
      "description": "Itemレベル（特殊処理）- 互換用（個別タイプに分割）"
    },
    "subject_label_square": {
      "id": "subject_label_square",
      "name": "括弧科目名（〔〕）",
      "patterns": [
        "^〔.+〕$"
      ],
      "examples": [
        "〔医療と社会〕",
        "〔人体の構造と機能〕"
      ],
      "description": "Itemレベル（特殊処理）- 〔〕で囲まれた科目名"
    },
    "subject_label_double_square": {
      "id": "subject_label_double_square",
      "name": "括弧科目名（【】）",
      "patterns": [
        "^【.+】$"
      ],
      "examples": [
        "【医療と社会】",
        "【人体の構造と機能】"
      ],
      "description": "Itemレベル（特殊処理）- 【】で囲まれた科目名"
    },
    "subject_label_corner": {
      "id": "subject_label_corner",
      "name": "括弧科目名（［］）",
      "patterns": [
        "^［.+］$"
      ],
      "examples": [
        "［善悪の判断，自律，自由と責任］"
      ],
      "description": "Itemレベル（特殊処理）- ［］で囲まれた科目名"
    },
    "subject_label_round": {
      "id": "subject_label_round",
      "name": "丸括弧見出し（（））",
      "patterns": [
        "^[（(].+[）)]$"
      ],
      "examples": [
        "（丸括弧見出し）",
        "（相談窓口の担当者が適切に対応することができるようにしていると認められる例）"
      ],
      "description": "Itemレベル（特殊処理）- （）で囲まれた見出しや説明文"
    },
    "article_boundary": {
      "id": "article_boundary",
      "name": "第○パターン",
      "patterns": [
        "^第[0-9０-９一二三四五六七八九十百千]+$"
      ],
      "examples": [
        "第１",
        "第２",
        "第一",
        "第二"
      ],
      "description": "Article（分割）"
    },
