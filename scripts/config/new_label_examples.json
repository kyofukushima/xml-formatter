{
  "new_label_definitions": {
    "closing_paren_fullwidth_lowercase_alphabet": {
      "id": "closing_paren_fullwidth_lowercase_alphabet",
      "name": "閉じ括弧全角小文字アルファベット",
      "patterns": [
        "^[ａ-ｚ]+[）)]$"
      ],
      "examples": [
        "ａ）",
        "ｂ）",
        "ｃ）",
        "ｄ）",
        "ｅ）",
        "ｆ）",
        "ｇ）",
        "ｈ）",
        "ｉ）",
        "ｊ）",
        "ｋ）",
        "ｌ）",
        "ｍ）",
        "ｎ）"
      ],
      "description": "閉じ括弧のみの全角小文字アルファベット（開き括弧なし）"
    },
    "closing_paren_fullwidth_number": {
      "id": "closing_paren_fullwidth_number",
      "name": "閉じ括弧全角数字",
      "patterns": [
        "^[０-９]+[）)]$"
      ],
      "examples": [
        "１）",
        "２）",
        "３）"
      ],
      "description": "閉じ括弧のみの全角数字（開き括弧なし）"
    },
    "hierarchical_number": {
      "id": "hierarchical_number",
      "name": "階層番号",
      "patterns": [
        "^[０-９]+[．.][０-９]+([．.][０-９]+)*$"
      ],
      "examples": [
        "４．１",
        "４．２",
        "４．２．１",
        "４．２．２",
        "４．２．３",
        "４．３",
        "４．３．１",
        "４．３．２",
        "４．３．３",
        "４．４",
        "４．４．１",
        "４．４．２"
      ],
      "description": "階層的な番号付け（数字+ピリオドの組み合わせ）"
    },
    "note_label": {
      "id": "note_label",
      "name": "注記ラベル",
      "patterns": [
        "^注記[０-９]+$"
      ],
      "examples": [
        "注記１",
        "注記２",
        "注記３"
      ],
      "description": "注記ラベル（注記+数字）"
    }
  },
  "pattern_priority_insertion_points": {
    "closing_paren_fullwidth_lowercase_alphabet": {
      "insert_after": "paren_fullwidth_lowercase_alphabet",
